{
    "search_keywords": [
        "extension:m3u8 in:file",
        "extension:m3u in:file",
       # "iptv playlist extension:m3u,m3u8 in:file",
      #  "raw.githubusercontent.com path:.m3u8",
      #  "raw.githubusercontent.com path:.m3u",
      #  "tv channels extension:m3u,m3u8 in:file",
      #  "live tv extension:m3u,m3u8 in:file",
     #   "playlist.m3u8 in:file",
      #  "index.m3u8 in:file",
      #  "channels.m3u in:file",
      #  "iptv links extension:m3u,m3u8 in:file"
    ],
    "per_page": 100,
    "max_search_pages": 5,
    "github_api_timeout": 20,
    "github_api_retry_wait": 10,
    "channel_fetch_timeout": 15,
    "channel_check_timeout": 6,
    "max_channel_urls_per_group": 200,
    "name_filter_words": [
        "购物", "理财", "导视", "指南", "测试", "芒果", "CGTN",
        "(480p)", "(360p)", "(240p)", "(406p)", " (540p)", "(600p)", "(576p)",
        "[Not 24/7]", "DJ", "音乐", "演唱会", "舞曲", "春晚", "格斗", "粤",
        "祝", "体育", "广播", "博斯", "神话"
    ],
    "url_filter_words": [
        "example.com/test",
        "badurl.xyz"
    ],
    "channel_name_replacements": {
        "FHD": "", "HD": "", "hd": "", "频道": "", "高清": "",
        "超清": "", "20M": "", "-": "", "4k": "", "4K": "", "4kR": ""
    },
    "ordered_categories": [
        "央视频道", "卫视频道", "湖南频道", "港台频道"
    ],
    "url_pre_screening": {
        "allowed_protocols": ["http", "https", "rtmp", "rtp", "p3p"],
        "stream_extensions": [".m3u8", ".flv", ".mp4", ".ts", ".aac", ".mp3", ".ogg", ".webm", ".avi", ".mov", ".wmv", ".rm", ".rmvb", ".3gp", ".mkv"],
        "invalid_url_patterns": [
            "example\\.com",
            "test\\.com",
            "localhost",
            "127\\.0\\.0\\.1",
            "\\.png$",
            "\\.jpg$",
            "\\.jpeg$",
            "\\.gif$",
            "\\.css$",
            "\\.js$",
            "\\.html$",
            "\\.xml$",
            "(\\s|%20)",
            "null",
            "invalid",
            "error",
            "blank"
        ]
    }
}
